{
  "consul": "consul:8500",
  "preStart": "/bin/reload-nginx.sh preStart" // BROKEN/DEMO bad JSON
  "services": [
    {
      "name": "nginx",
      "port": 80,
      "interfaces": ["eth1", "eth0"],
      "health": "/usr/bin/curl -o /dev/null -s http://localhost/404",
      "poll": 10,
      "ttl": 25
    }
  ],
  "backends": [
    {
      "name": "sales",
      "poll": 3,
      "onChange": "/bin/reload-nginx.sh onChange"
    },
    {
      "name": "customers",
      "poll": 4,
      "onChange": "/bin/reload-nginx.sh onChange"
    }
  ]
}
